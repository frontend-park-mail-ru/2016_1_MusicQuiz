<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
    <div class="container">
        <div class="container_header"></div>
        <div class="container_body">
        	<fest:each iterate="data.scores" index="i" value="v">
        		<div class="button"><h2><fest:value>v.name</fest:value><fest:space/>:<fest:space/><fest:value>v.score</fest:value></h2>
        	</div>
        	</fest:each>
            <a class="button" href='#main'><i class="fa fa-caret-left fa-lg"></i><fest:space/>Back</a>
        </div>
    </div>
</fest:template>
